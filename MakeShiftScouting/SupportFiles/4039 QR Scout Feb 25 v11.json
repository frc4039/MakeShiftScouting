{
    "$schema": "../schema.json",
    "title": "QRScout",
    "page_title": "Charged Up",
    "sections": [
        {
            "name": "Prematch",
            "preserveDataOnReset": true,
            "fields": [
                {
                    "code": "ev",
                    "title": "Event",
                    "type": "select",
                    "choices": {
                        "Georgian": "Georgian",
                        "Western": "Western",
                        "DCMP": "DCMP"
                    },
                    "defaultValue": "n",
                    "required": false
                },
                {
                    "title": "Scouter Name",
                    "type": "text",
                    "required": true,
                    "code": "scouter"
                },
                {
                    "title": "Match Number",
                    "type": "number",
                    "required": true,
                    "code": "matchNumber"
                },
                {
                    "title": "Team Number",
                    "type": "number",
                    "required": true,
                    "code": "teamNumber"
                },
                {
                    "title": "Robot",
                    "type": "select",
                    "required": true,
                    "code": "robot",
                    "choices": {
                        "r1": "Red 1",
                        "r2": "Red 2",
                        "r3": "Red 3",
                        "b1": "Blue 1",
                        "b2": "Blue 2",
                        "b3": "Blue 3"
                    },
                    "defaultValue": "r1"
                },
                {
                    "title": "No Show",
                    "type": "boolean",
                    "defaultValue": false,
                    "required": false,
                    "code": "noShow"
                }
            ]
        },
        {
            "name": "Autonomous",
